<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.sanchoy.bracualleviator.faculty_page"
    android:background="#ff323232"
    android:clickable="false">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Name : "
        android:id="@+id/textView9"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#141414"
        android:textStyle="bold"
        android:background="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Initial : "
        android:id="@+id/textView10"
        android:layout_below="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="12dp"
        android:textColor="#141414"
        android:textStyle="bold"
        android:background="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Department : "
        android:id="@+id/textView11"
        android:layout_below="@+id/textView10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="11dp"
        android:textColor="#141414"
        android:textStyle="bold"
        android:background="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="INSIDE THE ROOM?  : "
        android:textStyle="bold"
        android:id="@+id/textView12"
        android:layout_marginTop="42dp"
        android:textColor="#ffffffff"
        android:background="#ff080808"
        android:layout_below="@+id/textView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="status"
        android:id="@+id/roomStatus"
        android:layout_alignTop="@+id/textView12"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:checked="false"
        android:textColor="#ff02beb0" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=" CONSULTATION  REQUESTS "
        android:textStyle="bold"
        android:id="@+id/textView13"
        android:textColor="#ffff0008"
        android:background="#ff080808"
        android:layout_above="@+id/studentRequestsWithID"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="320dp"
        android:id="@+id/studentRequestsWithID"
        android:clickable="true"
        android:background="#fffe9a03"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="250dp"
        android:layout_height="23dp"
        android:id="@+id/FacultyName"
        android:layout_alignBottom="@+id/textView9"
        android:layout_toRightOf="@+id/textView9"
        android:layout_toEndOf="@+id/textView9"
        android:background="#ffffff" />

    <EditText
        android:layout_width="120dp"
        android:layout_height="23dp"
        android:id="@+id/facultyInitial"
        android:background="#ffffffff"
        android:layout_alignTop="@+id/textView10"
        android:layout_toRightOf="@+id/textView10"
        android:layout_toEndOf="@+id/textView10" />

    <EditText
        android:layout_width="72dp"
        android:layout_height="23dp"
        android:id="@+id/FacultyDepartment"
        android:background="#ffffff"
        android:layout_above="@+id/textView12"
        android:layout_toRightOf="@+id/textView11"
        android:layout_toEndOf="@+id/textView11" />

</RelativeLayout>
