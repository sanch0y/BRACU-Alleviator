<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.sanchoy.bracualleviator.student_page"
    android:background="#ff323232">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/txtVw"
        android:singleLine="false"
        android:textColor="#ffffc500"
        android:textSize="@dimen/abc_action_bar_progress_bar_size"
        android:typeface="serif"
        android:textStyle="bold"
        style="@style/Base.ThemeOverlay.AppCompat.Dark"
        android:textIsSelectable="false"
        android:text="WHAT  TO ALLEVIATE?"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#ff080808" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Faculty Status"
        android:id="@+id/goToFacultyStatus"
        android:layout_marginTop="114dp"
        android:clickable="true"
        android:onClick="fromStudentToFaculty"
        android:textColor="#ff0a95db"
        android:textSize="@dimen/abc_text_size_large_material"
        android:textStyle="bold"
        android:background="#ff000000"
        android:layout_below="@+id/txtVw"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
<!--
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Lab Status"
        android:id="@+id/goToLabStatus"
        android:textSize ="@dimen/abc_text_size_large_material"
        android:textStyle="bold"
        android:clickable="true"
        android:elegantTextHeight="true"
        android:onClick="fromStudentToLab"
        android:textColor="#ff0a95db"
        android:background="#ff000000"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/goToFacultyStatus"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
-->
</RelativeLayout>
